{
	"name": "Hey NSA!",
	"description": "A Chrome Extension to broadast your browser activity",
	"version": "1.0.0",
	"permissions": [
		"<all_urls>"
	],
	"background": {
		"page": "background.html",
		"persistent": false
	},
	"icons": {
		"16": "icon16x16.png",
		"48": "icon48x48.png",
		"128": "icon128x128.png"
	},
	"browser_action": {
		"default_title": "Start broadcasting!",
		"default_icon": {
			"19": "icon19x19.png",
			"38": "icon76x76.png"
		}
	},
	"manifest_version": 2,
	"content_security_policy": "script-src 'self' https://heynsa-server.herokuapp.com; object-src 'self'"
}